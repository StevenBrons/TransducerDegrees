  title     = {Classifying Non-periodic Sequences by Permutation Transducers},
  booktitle = {Developments in Language Theory},
  year      = {2017},
  publisher = {Springer International Publishing},
  address   = {Cham},
  pages     = {365--377},
  abstract  = {Transducers order infinite sequences into natural classes, but permutation transducers provide a finer classification, respecting certain changes to finite segments. We investigate this hierarchy for non-periodic sequences over {\$}{\$}{\backslash}{\{}0,1{\backslash}{\}}{\$}{\$}in which the groups of 0s and 1s grow according to simple functions like polynomials. In this hierarchy we find infinite strictly ascending chains of sequences, all being equivalent with respect to ordinary transducers.},
  isbn      = {978-3-319-62809-7}
}

